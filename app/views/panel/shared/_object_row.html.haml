%tr
  - if object.respond_to? :image
    %td= image_tag((object.image.file.thumbnail.url rescue ImageUploader::default_url), height: 80, class: 'img-rounded')
  %td
    %strong= link_to object.name, [:panel, object]
  = yield
  %td= link_to t('panel.views.edit'), [:edit, :panel, object], class: 'btn btn-primary'
  %td= link_to t('panel.views.remove'), [:panel, object], method: :delete, class: 'btn btn-danger', data: {confirm: t('panel.views.confirm')}
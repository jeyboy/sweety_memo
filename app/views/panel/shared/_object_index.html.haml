%h1= t('token.list', scope: name)

- if objects.present?
  = paginate objects, theme: 'twitter-bootstrap-3'

  %table.table.table-striped
    = render partial: "panel/#{name.pluralize}/#{name}", collection: objects

  = paginate objects, theme: 'twitter-bootstrap-3'
- else
  .text-center
    %h1= t('token.no_list', scope: name)
    = link_to t('token.add', scope: name), [:new, :panel, name.to_sym], class: 'btn btn-success'

